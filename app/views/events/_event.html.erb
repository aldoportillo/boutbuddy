<div id="<%= dom_id event %>">
  <p>
    <strong>Title:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= event.time %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= event.price %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= event.bio %>
  </p>

  <p>
    <strong>Venue:</strong>
    <%= event.venue_id %>
  </p>

</div>
