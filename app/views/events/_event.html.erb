<div id="<%= dom_id event %>">
  <p>
    <strong>Title:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= event.time.strftime("%a, %b %e, %Y") %>
  </p>

  <p>
    <strong>Time:</strong>
    <%= event.time.strftime("%l:%M %p") %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= event.price %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= event.bio %>
  </p>

  <p>
    <strong>Venue:</strong>
    <!--Get Params from here to fetch GMAPS API-->
    <%= event.venue.name %>
  </p>

  

</div>
